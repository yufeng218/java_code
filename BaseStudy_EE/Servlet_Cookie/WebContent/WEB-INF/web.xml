<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlet_Cookie</display-name>
  
  <servlet>
    <servlet-name>setCookie</servlet-name>
    <servlet-class>com.my.demo.CookieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getCookie</servlet-name>
    <servlet-class>com.my.demo.TestCookie</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginStart</servlet-name>
    <servlet-class>com.my.demo.PrepareLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.my.demo.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>setCookie</servlet-name>
    <url-pattern>/servlet/setCookie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getCookie</servlet-name>
    <url-pattern>/servlet/getCookie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginStart</servlet-name>
    <url-pattern>/servlet/loginStart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <!-- 
  <welcome-file-list>
    <welcome-file>show.html</welcome-file>
  </welcome-file-list>
   -->
</web-app>